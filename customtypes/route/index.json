{
  "format": "page",
  "id": "route",
  "label": "Fietsroute",
  "repeatable": true,
  "status": true,
  "json": {
    "Main": {
      "uid": {
        "type": "UID",
        "config": {
          "label": "UID"
        }
      },
      "title": {
        "type": "Text",
        "config": {
          "label": "Titel",
          "placeholder": "Titel"
        }
      },
      "distance": {
        "type": "Number",
        "config": {
          "label": "Afstand",
          "placeholder": "Afstand in km, noteer zo: 5.3"
        }
      },
      "body": {
        "type": "Text",
        "config": {
          "label": "Beschrijving",
          "placeholder": "Korte beschrijving"
        }
      },
      "download_route": {
        "type": "Link",
        "config": {
          "label": "Fietsknoop Route",
          "placeholder": "GPX van de Fietsknoop app toevoegen",
          "select": "media",
          "allowText": true
        }
      },
      "route": {
        "type": "Image",
        "config": {
          "label": "Route",
          "constraint": {
            "width": 600,
            "height": 400
          },
          "thumbnails": []
        }
      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {}
        }
      }
    }
  }
}
